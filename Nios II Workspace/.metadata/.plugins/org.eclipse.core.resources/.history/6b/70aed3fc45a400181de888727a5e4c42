#include <stdint.h>
#include "altera_avalon_tse.h"
#include "eth_init.h"

#define MDIO_ADDR 0x0A



int init_eth(uint8_t* mac){
	printf("I am ethernet!\n");

	uint32_t * eth_config_reg;
	eth_config_reg = ETH_BASE;

	printf("Eth reg %#08X \n", *eth_config_reg);
	printf("mac 0 reg %#08X \n", *(eth_config_reg + (32*3)));
	printf("mac 1 reg %#08X \n", *eth_config_reg);


	return 0;

}

